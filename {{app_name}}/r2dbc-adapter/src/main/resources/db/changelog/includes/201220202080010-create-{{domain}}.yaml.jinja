databaseChangeLog:
  - changeSet:
      id: create-table-t_{{domain}}
      author: Paul WILLIAMS
      changes:
        - createTable:
            tableName: T_{{domain_uppercase}}
            columns:
              - column:
                  name: TECH_ID
                  type: BIGINT
                  autoIncrement: true
                  constraints:
                    primaryKey: true
                    nullable: false
              - column:
                  name: CODE
                  type: BIGINT
                  constraints:
                    nullable: false
              - column:
                  name: DESCRIPTION
                  type: VARCHAR(255)
                  constraints:
                    nullable: false
      rollback:
        - dropTable:
            tableName: T_{{domain_uppercase}}

